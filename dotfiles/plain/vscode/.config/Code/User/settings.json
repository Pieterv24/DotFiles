{
    "editor.fontFamily": "'Ubuntu Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 16,
    "explorer.openEditors.visible": 0,
    "editor.renderLineHighlight": "gutter",
    "workbench.startupEditor": "welcomePage",
    "window.restoreWindows": "none",
    "window.titleBarStyle": "native",
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.fontFamily": "Noto Mono for Powerline",
    "editor.suggestSelection": "first",
    "git.autofetch": true,
    "files.associations": {
        "filelist": "shellscript"
    },
    "discord.lowerDetailsDebugging": "Debugging: {empty}",
    "discord.lowerDetailsEditing": "Workspace: {empty}",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "[java]": {
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "spotlessGradle.diagnostics.enable": true,
        "spotlessGradle.format.enable": true
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    }
}